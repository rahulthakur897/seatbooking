{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["react-app", "airbnb", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "no-console": "off",
        "react/prop-types": "off",
        "require-default-props": "off",
        "default-param-last": "off"
    }
}
